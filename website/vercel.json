{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/tietosuoja", "destination": "/tietosuoja.html" },
    { "source": "/kayttoehdot", "destination": "/kayttoehdot.html" },
    { "source": "/tuki", "destination": "/tuki.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
